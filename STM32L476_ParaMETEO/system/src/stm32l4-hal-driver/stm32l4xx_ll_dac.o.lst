 540:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h **** /**
 541:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   * @brief  Helper macro to calculate the DAC conversion data (unit: digital
 542:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   *         value) corresponding to a voltage (unit: mVolt).
 543:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   * @note   This helper macro is intended to provide input data in voltage
 544:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   *         rather than digital value,
 545:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   *         to be used with LL DAC functions such as
 546:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   *         @ref LL_DAC_ConvertData12RightAligned().
 547:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   * @note   Analog reference voltage (Vref+) must be either known from
 548:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   *         user board environment or can be calculated using ADC measurement
 549:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   *         and ADC helper macro @ref __LL_ADC_CALC_VREFANALOG_VOLTAGE().
 550:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   * @param  __VREFANALOG_VOLTAGE__ Analog reference voltage (unit: mV)
 551:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   * @param  __DAC_VOLTAGE__ Voltage to be generated by DAC channel
 552:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   *                         (unit: mVolt).
 553:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   * @param  __DAC_RESOLUTION__ This parameter can be one of the following values:
 554:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   *         @arg @ref LL_DAC_RESOLUTION_12B
 555:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   *         @arg @ref LL_DAC_RESOLUTION_8B
 556:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   * @retval DAC conversion data (unit: digital value)
 557:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   */
 558:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h **** #define __LL_DAC_CALC_VOLTAGE_TO_DATA(__VREFANALOG_VOLTAGE__,\
 559:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****                                       __DAC_VOLTAGE__,\
 560:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****                                       __DAC_RESOLUTION__)                      \
 561:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   ((__DAC_VOLTAGE__) * __LL_DAC_DIGITAL_SCALE(__DAC_RESOLUTION__)              \
 562:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****    / (__VREFANALOG_VOLTAGE__)                                                  \
 563:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   )
