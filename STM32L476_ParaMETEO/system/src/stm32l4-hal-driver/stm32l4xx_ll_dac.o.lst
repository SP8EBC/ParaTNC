 515:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****   (((__DECIMAL_NB__) == 1U)                                                    \
 516:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****     ? (                                                                        \
 517:../system/include/stm32l4-hal-driver/stm32l4xx_ll_dac.h ****        LL_DAC_CHANNEL_1                                                        \
